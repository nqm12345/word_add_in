{
  "name": "word-server-editor",
  "version": "1.0.0",
  "description": "Word Add-in for editing documents directly from server",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "start:mongo": "node server/server-mongodb.js",
    "start:webdav": "node server/webdav-mongodb.js",
    "start:all": "concurrently \"npm run start:mongo\" \"npm run start:webdav\"",
    "dev": "concurrently \"npm run dev-server\" \"npm run dev-client\"",
    "dev:mongo": "concurrently \"npm run dev-server:mongo\" \"npm run dev-client\"",
    "dev-server": "nodemon server/server.js",
    "dev-server:mongo": "nodemon server/server-mongodb.js",
    "dev-client": "office-addin-debugging start manifest.xml desktop"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-basic-auth": "^1.2.1",
    "mammoth": "^1.6.0",
    "mongodb": "^7.0.0",
    "multer": "^1.4.5-lts.1",
    "officegen": "^0.6.5",
    "webdav-server": "^2.6.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "office-addin-debugging": "^5.0.0",
    "office-addin-dev-certs": "^1.13.0"
  }
}
